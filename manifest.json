{
  "manifest_version": 2,
  "name": "favoritone",
  "version": "1.0",
  "description": "useful kintone portal widget for favorite",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.cybozu.com/k/"],
      "js": ["inject.js"]
    }
  ],
  "web_accessible_resources": ["dist/main.js"]
}